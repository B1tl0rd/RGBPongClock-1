{
"event": "weather_hook",
"url": "http://api.openweathermap.org/data/2.5/forecast/daily",
"requestType": "POST",
"headers": null,
"query": {
	"q": "{{mycity}}",
	"mode": "json",
	"units": "{{units}}",
	"cnt": 7,
	"appid": "{{apikey}}"
	},
"responseTemplate": "{{#list}}{{temp.day}}{{#weather}}~{{id}}~{{/weather}}{{/list}}",
"json": null,
"auth": null,
"noDefaults": true,
"devideid": null,
"mydevices": true
}